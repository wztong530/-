    loadMore(){\r\n      if(this.hasMore==false){\r\n        this.$toast(\"没有更多的数据了...\");\r\n        return;\r\n      }\r\n      this.n++;\r\n      var url=\"http://127.0.0.1:3000/getGoodList?pno=\"+this.n;\r\n      this.axios.get(url).then(res=>{\r\n        this.list=this.list.concat(res.data.data);\r\n        if(res.data.data.length<4){\r\n          this.showMore=false;\r\n        }\r\n        if(this.n>=res.data.pageCount){\r\n          this.hasMore=false;\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n  .app-goodlist{\r\n    padding-top:40px;\r\n    padding-bottom:50px;\r\n    overflow-x:hidden;\r\n    display:flex;\r\n    flex-wrap:wrap;\r\n    justify-content:space-between;\r\n  }\r\n  .app-goodlist .goods-item{\r\n    width: 49%;\r\n    border: 1px solid #ccc;\r\n    margin: 2px 0;\r\n    padding: 2px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    min-height: 245px;\r\n  }\r\n  .app-goodlist .goods-item img{\r\n    width: 100%;\r\n  }\r\n  .app-goodlist p{\r\n    font-size:18px;\r\n    text-align:center;\r\n    margin:10px auto;\r\n    width:100%;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src\\components\\home"}]}