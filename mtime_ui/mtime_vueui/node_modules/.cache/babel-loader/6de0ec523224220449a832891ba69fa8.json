          <!-- 1.复选框 -->\r\n                  <input type=\"checkbox\">\r\n                  <!-- 2.商品名称 -->\r\n                  {{item.lname}}\r\n                  <!-- 3.商品价格 -->\r\n                  ¥{{item.price}}\r\n                  <!-- 4.删除按钮 -->\r\n                  <input type=\"button\" value=\"删除\" @click=\"removeItem\" :data-id=\"item.id\">\r\n                </p>\r\n              </div>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <div class=\"mui-card-footer\">\r\n      删除选中商品\r\n    </div>\r\n  </div>\r\n  <my-footer></my-footer>\r\n</div>\r\n</template>\r\n<script>\r\nimport myFooter from \"@/components/Footer.vue\"\r\nexport default {\r\n  data(){\r\n    return{\r\n      list:[]\r\n    }\r\n  },\r\n  created(){\r\n    this.loadMore();\r\n  },\r\n  methods:{\r\n    selectAll(){\r\n      console.log()\r\n    },\r\n    removeItem(e){\r\n      //功能:删除指定商品\r\n      //1.获取商品编号\r\n      var id=e.target.dataset.id;\r\n      //2.创建变量url\r\n      var url=\"http://127.0.0.1:3000/removeCartItem?id=\"+id;\r\n      //3.发送ajax请求\r\n      this.axios.get(url).then(res=>{\r\n        //4.获取返回结果\r\n        //5.判断如果删除成功\r\n        if(res.data.code==1){\r\n          this.$toast(\"删除成功\");\r\n          //6.将当前商品对象删除\r\n          //数据库已经删除成功,网页内容显示新内容(删除后)\r\n          this.loadMore();\r\n        }else{\r\n          this.$toast(\"删除失败\");\r\n        }\r\n      })\r\n    },\r\n    loadMore(){\r\n      // 1.创建变量url\r\n      var url=\"http://127.0.0.1:3000/getCart\";\r\n      // 2.发送ajax请求\r\n      this.axios.get(url).then(res=>{\r\n        // 3.获取服务器返回的数据\r\n        //console.log(res.data.data);\r\n        // 4.保存list\r\n        this.list=res.data.data;\r\n        console.log(this.list);\r\n      })\r\n    }\r\n  },\r\n  components:{\r\n    myFooter\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src\\components\\tabbar"}]}